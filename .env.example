# E-Scooter Admin Dashboard Environment Variables

# API Configuration
REACT_APP_API_URL=http://localhost:3000/api
REACT_APP_ENVIRONMENT=development

# Weather API Configuration (OpenWeatherMap)
# Get your free API key from: https://openweathermap.org/api
REACT_APP_OPENWEATHER_API_KEY=b8c3f1a2e4d6f8a9c1b3e5d7f9a2c4e6

# Google Maps API (for enhanced map features)
# Get API key from: https://developers.google.com/maps/documentation/javascript/get-api-key
REACT_APP_GOOGLE_MAPS_KEY=your_google_maps_api_key_here

# Prayer Times API (for cultural considerations)
# Free API, no key required: https://aladhan.com/prayer-times-api
REACT_APP_PRAYER_TIMES_API=https://api.aladhan.com/v1

# Application Settings
REACT_APP_DEFAULT_CITY=Riyadh
REACT_APP_DEFAULT_COUNTRY=SA
REACT_APP_CURRENCY=SAR
REACT_APP_LANGUAGE=en

# Feature Flags
REACT_APP_ENABLE_LIVE_WEATHER=true
REACT_APP_ENABLE_PRAYER_TIMES=true
REACT_APP_ENABLE_ARABIC_SUPPORT=true

# Development Settings
REACT_APP_DEBUG_MODE=false
REACT_APP_MOCK_DATA=false
